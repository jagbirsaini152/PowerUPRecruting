  <Flowchart sap2010:Annotation.AnnotationText="This flow allows for processing a series of files, each having structured data inside them. The TransactionItem is a DataRow within a file.&#xA;&#xA;ARGUMENTS&#xA;- in_AgencyDataTable -Agency details Datatable; captures data from Sharepoint read during Init; global variable in Framework workflow.&#xA;- io_TransactionNumber - index of record in process; intialize to 1 during Init; is incremented by the Framework but reset here for each file.&#xA;- out_TransactionItem - data to be processed.&#xA;&#xA;STEPS&#xA;- Check for the data table and if exists then for next available record&#xA;- return next transaction Item or Nothing " DisplayName="GetTransactionData_NewFlow" sap2010:WorkflowViewState.IdRef="Flowchart_1" StartNode="{x:Reference __ReferenceID4}">
    <FlowDecision x:Name="__ReferenceID4" sap2010:Annotation.AnnotationText="Determine if the Datatable is not null." Condition="[in_TransactionData is Nothing]" DisplayName="More Files To Process?" sap2010:WorkflowViewState.IdRef="FlowDecision_2">
      <FlowDecision.True>
        <FlowStep x:Name="__ReferenceID0" sap2010:WorkflowViewState.IdRef="FlowStep_1">
          <Assign sap2010:Annotation.AnnotationText="No Rows in Datatable. Set TransactionItem to Nothing, which will stop transaction processing and go to End." sap2010:WorkflowViewState.IdRef="Assign_5">
            <Assign.To>
              <OutArgument x:TypeArguments="sd:DataRow">[out_TransactionItem]</OutArgument>
            </Assign.To>
            <Assign.Value>
              <InArgument x:TypeArguments="sd:DataRow">[Nothing]</InArgument>
            </Assign.Value>
          </Assign>
        </FlowStep>
      </FlowDecision.True>
      <FlowDecision.False>
        <FlowDecision x:Name="__ReferenceID1" sap2010:Annotation.AnnotationText="Checks if there are more DataRows in the DataTable from the file currently being processed" Condition="[io_TransactionNumber &lt;= in_TransactionData.Rows.Count]" DisplayName="More Records in Current File?" sap2010:WorkflowViewState.IdRef="FlowDecision_1">
          <FlowDecision.True>
            <FlowStep x:Name="__ReferenceID2" sap2010:WorkflowViewState.IdRef="FlowStep_2">
              <Assign sap2010:Annotation.AnnotationText="Set output data for next transaction. Adjust indexes for the fact that counters use a 1-n range but data structures use a 0-n range." sap2010:WorkflowViewState.IdRef="Assign_6">
                <Assign.To>
                  <OutArgument x:TypeArguments="sd:DataRow">[out_TransactionItem]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="sd:DataRow">[in_TransactionData.Rows(io_TransactionNumber-1)]</InArgument>
                </Assign.Value>
              </Assign>
            </FlowStep>
          </FlowDecision.True>
          <FlowDecision.False>
            <FlowStep x:Name="__ReferenceID3" sap2010:WorkflowViewState.IdRef="FlowStep_3">
              <Assign sap2010:Annotation.AnnotationText="No more files. Set TransactionItem to Nothing, which will stop transaction processing and go to End." sap2010:WorkflowViewState.IdRef="Assign_7">
