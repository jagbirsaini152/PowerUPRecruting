  <Sequence sap2010:Annotation.AnnotationText="This flow processes transactions stored pulled from Outlook. Each message in the Inbox folderis a transaction.&#xA;&#xA;ARGUMENTS&#xA;- in_TransactionData -DataTable list of transaction records&#xA;- in_EmailAccount - name of the email/AD account (note: complete email address; case sensitive)&#xA;- io_TransactionNumber - index of record in process; intialize to 1 during Init; is incremented by the Framework but reset here for each file.&#xA;- out_TransactionItem - next data record to be processed.&#xA;&#xA;STEPS&#xA;- Check for the data table and if exists then for next available record&#xA;- return next transaction Item or Nothing" DisplayName="GetTransactionData" sap2010:WorkflowViewState.IdRef="Sequence_2">
    <Sequence.Variables>
      <Variable x:TypeArguments="scg:List(snm:MailMessage)" Name="ExtractedEmail" />
    </Sequence.Variables>
    <Sequence sap2010:WorkflowViewState.IdRef="Sequence_3">
      <ui:LogMessage DisplayName="Log Message" sap2010:WorkflowViewState.IdRef="LogMessage_2" Level="Info" Message="Getting Outlook mail Message.." />
      <ui:GetOutlookMailMessages Filter="{x:Null}" Account="[in_EmailAccount]" sap2010:Annotation.AnnotationText="Get the first email" DisplayName="Get Outlook Mail Messages" GetAttachements="False" sap2010:WorkflowViewState.IdRef="GetOutlookMailMessages_1" MailFolder="Inbox" MarkAsRead="True" Messages="[ExtractedEmail]" OnlyUnreadMessages="True" TimeoutMS="5000" Top="1" />
      <ui:LogMessage DisplayName="Log Message" sap2010:WorkflowViewState.IdRef="LogMessage_3" Level="Info" Message="Mail Message extracted." />
      <ui:ForEach x:TypeArguments="snm:MailMessage" CurrentIndex="{x:Null}" DisplayName="For Each" sap2010:WorkflowViewState.IdRef="ForEach`1_1" Values="[ExtractedEmail]">
        <ui:ForEach.Body>
          <ActivityAction x:TypeArguments="snm:MailMessage">
            <ActivityAction.Argument>
              <DelegateInArgument x:TypeArguments="snm:MailMessage" Name="message" />
            </ActivityAction.Argument>
            <Sequence DisplayName="Body" sap2010:WorkflowViewState.IdRef="Sequence_1">
              <Sequence.Variables>
                <Variable x:TypeArguments="x:String" Name="Attach" />
                <Variable x:TypeArguments="x:String" Name="FolderPath" />
              </Sequence.Variables>
              <Assign sap2010:WorkflowViewState.IdRef="Assign_7">
                <Assign.To>
                  <OutArgument x:TypeArguments="snm:MailMessage">[out_TransactionItem]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="snm:MailMessage">[message]</InArgument>
                </Assign.Value>
              </Assign>
            </Sequence>
          </ActivityAction>
        </ui:ForEach.Body>
      </ui:ForEach>
    </Sequence>
